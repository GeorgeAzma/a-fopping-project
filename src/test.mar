fn sum_n n
    ret n * (n + 1) / 2

fn factorial n
    f = 1
    for 2 => n i
        f *= i
    ret f

fn gcd a b
    while b != 0
        t = b
        b = a % b
        a = t
    ret a

fn rev_num n
    r = 0
    while n > 0
        r = r * 10 + n % 10
        n /= 10
    ret r

fn is_prime n
    divs = 0
    for 1 => n i
        if n % i == 0
            divs += 1
            if divs > 2
                ret 0
    ret divs == 2